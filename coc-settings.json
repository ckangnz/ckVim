{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "snippets.extends": {
    "html": ["html"],
    "css": ["css"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript", "javascript-jasmine-arrow"],
    "typescript": ["javascript", "javascript-jasmine-arrow"],
    "cs": ["cs"],
    "python": ["django"]
  },
  "eslint": {
    "enable": true,
    "filetypes": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  },
  "prettier": {
    "eslintIntegration": true,
    "trailingComma": "all",
    "singleQuote": true,
    "disableSuccessMessage": true
  },
  "omnisharp": {
    "trace.server": "information",
    "debug.server": true
  },
  "diagnostic": {
    "enableMssage": "jump",
    "errorSign": "",
    "warningSign": ""
  }
}
